{
  "name": "foundry-sandbox",
  "version": "1.0.0",
  "description": "MCP Server for running Foundry commands in a Docker-based sandbox",
  "type": "module",
  "main": "dist/src/index.js",
  "bin": {
    "foundry-sandbox": "./dist/src/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/index.js",
    "test": "yarn build && node --test dist/tests/**/*.test.js"
  },
  "keywords": [
    "mcp",
    "foundry",
    "docker",
    "ethereum",
    "solidity"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.47",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
